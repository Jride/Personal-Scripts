alias usedevscripts='source "$DEV_SCRIPTS/Aliases"; source "$ITV_DEV_SCRIPTS/Aliases"'
alias uselivescripts='source "$SCRIPTS/Aliases"; source "$ITV_SCRIPTS/Aliases"'

alias editscripts='subl "$DEV_SCRIPTS" && cd "$DEV_SCRIPTS"'
alias edititvscripts='subl "$ITV_DEV_SCRIPTS" && cd "$ITV_DEV_SCRIPTS"'
